{
  "schemaVersion": "0.6",
  "fields": [
    {
      "name": "foo"
    },
    {
      "name": "bar"
    }
  ],
  "rules": [
    {
      "constraints": [
        {
          "not": {
            "field": "foo",
            "is": "null"
          }
        },
        {
          "not": {
            "field": "bar",
            "is": "null"
          }
        },
        {
          "field": "foo",
          "is": "ofType",
          "value": "datetime"
        },
        {
          "field": "bar",
          "is": "ofType",
          "value": "datetime"
        },
        {
          "field": "bar",
          "is": "equalTo",
          "otherField": "foo",
          "offset": -3,
          "offsetUnit": "working days"
        }
      ]
    }
  ]

}