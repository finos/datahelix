{
  "schemaVersion": "0.15",
  "fields": [
    {
      "name": "tradeId",
      "type": "integer",
      "nullable": false,
      "unique": true
    },
    {
      "name": "stock",
      "type": "string",
      "nullable": false
    },
    {
      "name": "side",
      "type": "string",
      "nullable": false
    },
    {
      "name": "tradePrice",
      "type": "decimal",
      "nullable": false
    },
    {
      "name": "volume",
      "type": "integer",
      "nullable": false
    },
    {
      "name": "tradeDatetime",
      "type": "datetime",
      "nullable": false
    }
  ],
  "rules": [
    {
      "rule": "tradeId",
      "constraints": [
        { "field": "tradeId", "greaterThanOrEqualTo": 10000 },
        { "field": "tradeId", "lessThanOrEqualTo": 99999 }
      ]
    },
    {
      "rule": "stock",
      "constraints": [
        { "field": "stock", "inSet": "examples/demo-trades/stockCodes.csv" }
      ]
    },
    {
      "rule": "side",
      "constraints": [
        { "field": "side", "inSet": [ "BUY", "SELL" ] }
      ]
    },
    {
      "rule": "tradePrice",
      "constraints": [
        { "field": "tradePrice", "granularTo": 0.01 },
        { "field": "tradePrice", "greaterThan": 140.0 },
        { "field": "tradePrice", "lessThan": 150.0 }
      ]
    },
    {
      "rule": "volume",
      "constraints": [
        { "field": "volume", "greaterThanOrEqualTo": 1 },
        { "field": "volume", "lessThanOrEqualTo": 200 }
      ]
    },
    {
      "rule": "tradeDatetime",
      "constraints": [
        { "field": "tradeDatetime", "after": "2019-09-02T09:30:00.000" },
        { "field": "tradeDatetime", "before": "2019-09-02T16:30:00.000" }
      ]
    }
  ]
}